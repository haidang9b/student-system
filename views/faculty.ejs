<h1 class="my-5 text-primary text-center text-uppercase">Danh sách Khoa/Phòng ban</h1>
<div class="row">
    <% for(var i = 0; i< listFaculty.length; i++) { %>
        <div class="col-6 col-sm-4 col-md-3 col-lg-2">
            <div class="card card-faculty">
                <img class="card-img-top" src="<%=listFaculty[i].image%>" alt="Image of tdt" height="150" >
                <div class="card-body">
                    <h5 class="card-title name-faculty-card"><a href="/notifications/faculty/<%=listFaculty[i]._id%>"><%=listFaculty[i].name%></a></h5>
                </div>
            </div>
        </div>
    <% } %>
    
</div>